{
    "name": "finvizor",
    "version": "1.2.2",
    "description": "Get finviz stock data in proper format",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "build": "tsc --build",
        "test": "jest --config jestconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://samgozman@github.com/samgozman/finvizor.git"
    },
    "keywords": [
        "stock",
        "financial",
        "finviz"
    ],
    "author": "Sam Gozman <sam@gozman.space>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/samgozman/finvizor/issues"
    },
    "homepage": "https://github.com/samgozman/finvizor#readme",
    "dependencies": {
        "cheerio": "^1.0.0-rc.12",
        "got": "^11.8.2",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@types/jest": "^28.1.2",
        "@types/lodash": "^4.14.182",
        "eslint": "^8.15.0",
        "jest": "^28.1.0",
        "ts-jest": "^28.0.2",
        "typescript": "^5.4.3"
    },
    "files": [
        "lib/**/*"
    ],
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.test\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "testEnvironment": "node",
        "moduleDirectories": [
            "node_modules",
            "src"
        ]
    }
}
